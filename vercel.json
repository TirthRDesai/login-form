{
  "version": 2,
  "name": "LOGINPAGE",
  "builds": [
    {
      "src": "LOGINFORM/wsgi.py",
      "use": "@ardnt/now-python-wsgi",
      "config": {
        "maxLambdaSize": "30mb", "runtime" : "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "LOGINFORM/wsgi.py"
    }
  ]
}